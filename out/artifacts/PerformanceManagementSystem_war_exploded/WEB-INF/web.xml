<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.wk.system.web.servlet.LoginServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet2</servlet-name>
        <servlet-class>com.wk.system.web.servlet.LoginServlet2</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LoginServlet2</servlet-name>
        <url-pattern>/loginServlet2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.wk.system.web.servlet.RegisterServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet2</servlet-name>
        <servlet-class>com.wk.system.web.servlet.RegisterServlet2</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>RegisterServlet2</servlet-name>
        <url-pattern>/registerServlet2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.wk.system.web.servlet.LogoutServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logoutServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserInfoServlet</servlet-name>
        <servlet-class>com.wk.system.web.servlet.UserInfoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UserInfoServlet</servlet-name>
        <url-pattern>/userInfoServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UserPerformanceInfoServlet</servlet-name>
        <servlet-class>com.wk.system.web.servlet.UserPerformanceInfoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UserPerformanceInfoServlet</servlet-name>
        <url-pattern>/userPerformanceInfoServlet</url-pattern>
    </servlet-mapping>

        <servlet-name>AdminInfoServlet</servlet-name>
        <servlet-class>com.wk.system.web.servlet.AdminInfoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminInfoServlet</servlet-name>
        <url-pattern>/adminInfoServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>AdminInfosServlet</servlet-name>
        <servlet-class>com.wk.system.web.servlet.AdminInfosServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminInfosServlet</servlet-name>
        <url-pattern>/adminInfosServlet</url-pattern>
    </servlet-mapping>


</web-app>